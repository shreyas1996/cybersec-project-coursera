{
    "jwt": {
        "secret": {
            "doc": "jwt secret", 
            "default": "secret",
            "env": "JWT_SECRET"
        },
        "expiresIn": "1h",
        "encryption_enabled": {
            "default": false,
            "env": "JWT_ENCRYPTION_ENABLED"
        }
    },
    "aes": {
        "secret": {
            "doc": "aes secret", 
            "default": "secret",
            "env": "AES_SECRET"
        }
    }
}